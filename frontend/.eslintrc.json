{
   "env": {
      "browser": true,
      "es2021": true,
      "node": true
   },
   "extends": ["eslint:recommended", "plugin:react/recommended"],
   "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
   },
   "rules": {
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off",
      "react/display-name": "off"
   },
   "plugins": ["prettier"]
}
